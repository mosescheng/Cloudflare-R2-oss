{
  "name": "r2-dashboard",
  "version": "1.0.0",
  "description": "Cloudflare R2 文件管理后台",
  "main": "server.js",
  "scripts": {
    "start": "node server-env.js",
    "dev": "nodemon server-env.js",
    "start-original": "node server.js",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:pages": "wrangler pages deploy dist --config wrangler.pages.toml",
    "pages:dev": "wrangler pages dev dist --config wrangler.pages.toml",
    "wrangler:dev": "wrangler dev",
    "build": "./build.sh",
    "build:pages": "mkdir -p dist && echo 'Pages build complete'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "hono": "^3.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "wrangler": "^3.22.0"
  },
  "keywords": [
    "cloudflare",
    "r2",
    "file-management",
    "dashboard"
  ],
  "author": "Mannix",
  "license": "MIT"
}
